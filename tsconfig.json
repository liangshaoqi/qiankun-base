{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [ "src/*" ],
      "@assets/*": [ "src/assets/*" ],
      "@components/*": [ "src/components/*" ],
      "@layout/*": [ "src/layout/*" ],
      "@routes/*": [ "src/routes/*" ],
      "@utils/*": [ "src/utils/*" ],
      "@pages/*": [ "src/pages/*" ],
      "@request/*": [ "src/request/*" ],
      "@config/*": [ "src/config/*" ],
      "@api/*": [ "src/api/*" ],
    },
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src"
  ]
}
